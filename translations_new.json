{
    "version": 1,
    "carbon": {
        "GET": {
            "apiPrefix": "get",
            "baseURL": "https://api.carbonintensity.org.uk/",
            "microbitQueryString": "/%endpoint%/%unit?%",
            "unit": {
                "index": {
                    "urlPart": "intensity",
                    "returns": [
                        {
                            "name": "index",
                            "type": "string"
                        }
                    ]
                },
                "value": {
                    "urlPart": "intensity",
                    "returns": [
                        {
                            "name": "index",
                            "type": "string"
                        }
                    ]
                },
                "genmix": {
                    "urlPart": "generation",
                    "returns": [
                        {
                            "name": "index",
                            "type": "string"
                        }
                    ]
                }
            }
        }
    }
}